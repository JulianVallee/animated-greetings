{
  "categories": [
    {
      "title": "## Features",
      "labels": [
        "feature"
      ]
    },
    {
      "title": "## Fixes",
      "labels": [
        "fix"
      ]
    },
    {
      "title": "## Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## Chore",
      "labels": [
        "chore"
      ]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>\n\n View [full changelog](https://github.com/cnumr/ecoindex_python/compare/${{FROM_TAG}}...${{TO_TAG}})",
  "pr_template": "- ${{TITLE}} (PR #${{NUMBER}})",
  "empty_template": "- No Changes",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}